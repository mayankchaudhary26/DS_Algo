// sum of all sub arrays
#include<bits/stdc++.h>
#include<climits>
#include<iostream>
using namespace std;


int main(){
	
	int n;
	cout<<"enter the n value: "<<endl;
	cin>>n;

	int arr[n];
	for(int i=0; i<n; i++){
		cin>>arr[i];

	}
	int sum = 0;
	for(int i=0; i<n; i++){
		sum = 0;
		for(int j=i; j<n; j++){
			sum+= arr[j];
			cout<<sum<<endl;
		}
	}
	return 0;

	}
